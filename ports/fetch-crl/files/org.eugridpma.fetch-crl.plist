<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
        <key>Label</key>
        <string>org.eugridpma.fetch-crl</string>
        <key>UserName</key>
        <string>root</string>
        <key>ProgramArguments</key>
        <array>
                <string>/opt/local/sbin/fetch-crl</string>
		<string>-q</string>
		<string>--infodir</string>
		<string>/opt/local/etc/grid-security/certificates</string>
		<string>--statedir</string>
		<string>/opt/local/var/cache/fetch-crl</string>
		<string>--nosymlinks</string>
		<string>--agingtolerance</string>
		<string>24</string>
        </array>
	<key>StartInterval</key>
	<integer>21600</integer>
</dict>
</plist>
